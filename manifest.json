

{
    "name": "Opensea extension",
    "description": "Opensea Extension!",
    "version": "0.0.3",
    "manifest_version": 2,
    "homepage_url": "https://www.myetherwallet.com/",
    "permissions": [
      "storage",
      "activeTab",
      "scripting",
      "clipboardWrite",
      "notifications",
      "tabs"
    ],
    "background": {
      "page": "bg/background.html",
      "persistent": false
    },
    "browser_action": {
      "default_popup": "/pages/login.html"
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}